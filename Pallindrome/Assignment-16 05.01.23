public class Test {

	public static void main(String[] args) {
		String  str="biswa";
		String rev=" ";
		for(int i=str.length()-1;i>=0;i--)
		{
			rev=rev+str.charAt(i);
		}
		if(str.equals(rev))
		{
			System.out.println("This is a Palindrome String");
		}
		else {
			System.out.println("This is not a Palindrome String");
		}
	

	}

}
